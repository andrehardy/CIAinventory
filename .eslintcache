tName\">\r\n                        <Form.Label>Product Name:</Form.Label>\r\n                        <Form.Control type=\"text\" placeholder=\"Enter Product Name\" name=\"product_name\" />\r\n                    </Form.Group>\r\n\r\n                    <Form.Group controlId=\"formBasicPrice\">\r\n                        <Form.Label>Price:</Form.Label>\r\n                        <Form.Control type=\"number\" placeholder=\"Price in Euro\" name=\"price\" />\r\n                    </Form.Group>\r\n\r\n                    <Form.Group controlId=\"formBasicQty\">\r\n                        <Form.Label>Quantity:</Form.Label>\r\n                        <Form.Control type=\"number\" placeholder=\"How many: qty\" name=\"qty\" />\r\n                    </Form.Group>\r\n\r\n                    <Button variant=\"primary\" type=\"submit\">\r\n                        Add to Inventory\r\n            </Button>\r\n                </Form>\r\n                <Table striped bordered hover variant=\"dark\">\r\n                    <thead>\r\n                        <tr>\r\n                            <th>Index</th>\r\n                            <th>Product Name:</th>\r\n                            <th>Price</th>\r\n                            <th>Qty</th>\r\n                            <th>Options</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        {\r\n                            this.state.products.map((item, index) => {\r\n                                return (\r\n                                    <tr key={index}>\r\n                                        <td>{index}</td>\r\n                                        <td>{item.product_name}</td>\r\n                                        <td>{item.price}</td>\r\n                                        <td>{item.qty}</td>\r\n                                        <td>\r\n                                            <Button variant=\"success\" onClick={event => this.increQty(event)} value={index}>+</Button>\r\n                                            <Button variant=\"danger\" onClick={event => this.decreQty(event)} value={index}>-</Button>\r\n                                        </td>\r\n                                    </tr>\r\n                                )\r\n                            })\r\n                        }\r\n                    </tbody>\r\n                </Table>\r\n            </div>\r\n        )\r\n    }\r\n\r\n}","C:\\Users\\ahard\\Desktop\\InventoryApp\\easyInventory\\ReactProjects\\productInventory\\CIAinventory\\src\\components\\funcComponent.js",[],{"ruleId":"28","replacedBy":"29"},{"ruleId":"30","replacedBy":"31"},{"ruleId":"32","severity":1,"message":"33","line":32,"column":9,"nodeType":"34","endLine":32,"endColumn":42},{"ruleId":"32","severity":1,"message":"33","line":40,"column":9,"nodeType":"34","endLine":40,"endColumn":42},"no-native-reassign",["35"],"no-negated-in-lhs",["36"],"react/no-direct-mutation-state","Do not mutate state directly. Use setState().","MemberExpression","no-global-assign","no-unsafe-negation"]